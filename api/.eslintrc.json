{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "class-methods-use-this": "off",
      "no-unused-vars": "off",
      "consistent-return": "off",
      "no-extra-boolean-cast": "off",
      "camelcase": "off"
    }
}
